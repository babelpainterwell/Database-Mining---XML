library(RMySQL)
library(DBI)
# Connect to the MySQL database
con_mysql <- dbConnect(RMySQL::MySQL(),
dbname = "practicum2db",
host = "localhost",
user = "root",
password = "Zzw@1299123")
# Top ten authors with the most publications
query_top_authors <- "SELECT author_name, num_articles
FROM author_fact
ORDER BY num_articles DESC
LIMIT 10;"
top_authors <- dbGetQuery(con_mysql, query_top_authors)
cat("## Top Ten Authors with the Most Publications\n")
cat("Author Name | Number of Publications\n")
cat("---|---\n")
for (i in 1:nrow(top_authors)) {
cat(top_authors$author_name[i], "|", top_authors$num_articles[i], "\n")
}
# Top journal with the most articles per year
query_top_journal <- "SELECT journal_name, year, SUM(num_articles) as total_articles
FROM journal_fact
GROUP BY journal_name, year
ORDER BY total_articles DESC
LIMIT 1;"
top_journal <- dbGetQuery(con_mysql, query_top_journal)
cat("## Top Journal with the Most Articles per Year\n")
cat("Journal Name | Year | Number of Articles\n")
cat("---|---|---\n")
cat(top_journal$journal_name[1], "|", top_journal$year[1], "|", top_journal$total_articles[1], "\n")
